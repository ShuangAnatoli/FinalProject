<%- include('partials/header') %>
<%- include('partials/sidenav') %>
<!-- user -->
<main class="mt-5 pt-3">
    <h2 class="ms-2">All Achievements</h2>
    <% achievements.forEach(function(achievement){ %>
    <div class="d-flex">
        <div class="flex-shrink-0">
            <img src="https://i.pinimg.com/564x/ad/3a/4c/ad3a4c205ca5b86d0aebd3393138aecf.jpg" alt="" width="150px" height="150px">
            <!-- <i class="bi bi-trophy-fill"></i> -->
        </div>
        <div class="flex-grow-1 ms-3">
            
                <p>Name: <%= achievement.name %></p>
                <p>Conditions:<%= achievement.clearConditions %></p>
                <p>Type: <%= achievement.tag %></p>
           
        </div>
      </div>
      <hr>
      <% }); %>

</main>

<%- include('partials/sidenav') %>

</body>
</html>


